{
  "suggest.noselect": false,
  "diagnostic.displayByAle": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "typescript.tsx",
    "graphql",
    "json"
  ],
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod",
      ],
      "trace.server": "verbose",
      "filetypes": [
        "go"
      ]
    }
  }
}
